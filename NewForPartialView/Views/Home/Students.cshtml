@model IEnumerable<NewForPartialView.Models.Student>
@{
    ViewData["Title"] = "Students";
}

<h1>Students</h1>
<table class="table">

    <thead>
    <tr>
        <th scope="col">ID</th>
        <th scope="col">Name</th>
    </tr>
</thead>
</table>
<div class="container">
    <div class="row">
        @foreach(var item in Model)
        {
             <div>
                    <partial name="_StudentsPartialView" model="item">
             </div>
        }
    </div>

</div>

